<Window x:Class="test1.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test1"
        mc:Ignorable="d"
        Title="StartWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Start_image/startwindow.png"/>
        </Grid.Background>
        <TextBox x:Name="UserNameTextBox" HorizontalAlignment="Left" Margin="299,288,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="193" Height="42" TextAlignment="Center"/>
        <Button x:Name="StartButton" Content="" HorizontalAlignment="Left" Margin="299,217,0,0" VerticalAlignment="Top" Height="53" Width="193" Click="StartButton_Click_1" BorderBrush="{x:Null}">
            <Button.Background>
                <ImageBrush ImageSource="Start_image/next.png" Stretch="UniformToFill"/>
            </Button.Background>
        </Button>

    </Grid>
</Window>
